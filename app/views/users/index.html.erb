<%= render 'books/list', book: @book, user: @user %>

<h1>Users</h1>
<% @users.each do |user| %>
  <p>image: <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></p>
  <p>name: <%= user.name %></p>
  <%= link_to "show", user_path(user.id) %>

<% end %>
